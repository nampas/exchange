{
    "rules": {
        "quotes": [
            2,
            "single"
        ],
        "prettier/prettier": ["error", { "singleQuote": true }]
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 9,
    },
    "extends": ["prettier", "eslint-config-prettier"],
    "plugins": ["prettier"]
}
